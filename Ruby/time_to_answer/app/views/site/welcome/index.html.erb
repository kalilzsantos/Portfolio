<h1>Site::Welcome#index</h1>
<p>Find me in app/views/site/welcome/index.html.erb</p>

<%= link_to 'Área Administrativa', admins_backoffice_welcome_index_path %><br>
<%= link_to 'Área de Usuários', users_backoffice_welcome_index_path %><br>  


<br>
<% if admin_signed_in? %>
  <p>Administrador está logado</p>
  <p><%= current_admin.email %></p>
<% else %>
  <p>Administrador não está logado</p>
<% end %>
<br>
<% if user_signed_in? %>
  <p>Usuário está logado</p>
  <p><%= current_user.email %></p>
<% else %>
  <p>Usuário não está logado</p>  
<% end %>